<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <listener>
    <listener-class>listener.HelloListener</listener-class>
  </listener>
  <filter>
    <filter-name>EncodFilter</filter-name>
    <filter-class>filter.EncodFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>EncodFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TopicServlet</servlet-name>
    <servlet-class>servlet.TopicServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NewServlet</servlet-name>
    <servlet-class>servlet.NewServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PageServlet</servlet-name>
    <servlet-class>servlet.PageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NewListServlet</servlet-name>
    <servlet-class>servlet.NewListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NewsModifyServlet</servlet-name>
    <servlet-class>servlet.NewsModifyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NewsAddServlet</servlet-name>
    <servlet-class>servlet.NewsAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TopicListServlet</servlet-name>
    <servlet-class>servlet.TopicListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TopicModifyServlet</servlet-name>
    <servlet-class>servlet.TopicModifyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TopicServlet</servlet-name>
    <url-pattern>/newspages/TopicServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewServlet</servlet-name>
    <url-pattern>/newspages/NewServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PageServlet</servlet-name>
    <url-pattern>/PageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/newspages/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewListServlet</servlet-name>
    <url-pattern>/newspages/NewListServlet</url-pattern>
    <url-pattern>/NewListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewsModifyServlet</servlet-name>
    <url-pattern>/newspages/NewsModifyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewsAddServlet</servlet-name>
    <url-pattern>/newspages/NewsAddServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TopicListServlet</servlet-name>
    <url-pattern>/newspages/TopicListServlet</url-pattern>
    <url-pattern>/TopicListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TopicModifyServlet</servlet-name>
    <url-pattern>/TopicModifyServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <display-name>test</display-name>
    <filter-name>test</filter-name>
    <filter-class>filter.test</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>test</filter-name>
    <url-pattern>/test</url-pattern>
  </filter-mapping>
</web-app>